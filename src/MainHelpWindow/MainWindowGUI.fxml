<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainHelpWindow.MainController">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane fx:id="pane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="592.0" prefWidth="778.0">
         <children>
            <ComboBox fx:id="ctcDeparture" layoutX="14.0" layoutY="31.0" prefHeight="38.0" prefWidth="162.0" promptText="Choose the city" />
            <ComboBox fx:id="ctcArrival" layoutX="194.0" layoutY="31.0" prefHeight="38.0" prefWidth="173.0" promptText="Choose the city" />
            <Label layoutX="51.0" layoutY="14.0" prefHeight="17.0" prefWidth="88.0" text="Departure" />
            <Label layoutX="267.0" layoutY="14.0" text="Arrival" />
            <Label layoutX="406.0" layoutY="14.0" text="Delay (minutes)" />
            <Button fx:id="HelpButton" layoutX="660.0" layoutY="31.0" mnemonicParsing="false" onAction="#showHelp" prefHeight="38.0" prefWidth="88.0" text="Help" />
            <Button fx:id="findPath" layoutX="544.0" layoutY="31.0" mnemonicParsing="false" onAction="#callDijkstra" prefHeight="38.0" prefWidth="88.0" text="Find" />
            <Spinner fx:id="delaySpinner" layoutX="380.0" layoutY="38.0" />
            <Circle fill="DODGERBLUE" layoutX="273.0" layoutY="215.0" radius="19.0" stroke="BLACK" strokeType="INSIDE" />
            <Label fx:id="expectedTime" layoutX="549.0" layoutY="81.0" prefHeight="17.0" prefWidth="82.0" text="Expected Time" />
         </children></Pane>
    </children>
</GridPane>
